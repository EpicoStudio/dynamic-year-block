{
	"name": "dynamic-year-block",
	"version": "1.0.0",
	"description": "Display a copyright notice in your footer with the current year.",
	"author": "Marcio Duarte",
	"license": "GPL-2.0-or-later",
	"homepage": "https://epico.studio",
	"main": "build/index.js",
	"scripts": {
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"make-pot": "wp i18n make-pot . languages/awp.pot --slug=awp --domain=awp --exclude=node_modules,src",
		"make-json": "wp i18n make-json languages/ --no-purge",
		"plugin-zip": "wp-scripts plugin-zip",
		"build": "wp-scripts build --blocks-manifest",
		"start": "wp-scripts start --blocks-manifest"
	},
	"devDependencies": {
		"@wordpress/browserslist-config": "^6.21.0",
		"@wordpress/scripts": "^30.14.1"
	}
}
